#/bin/bash 
clear
chmod +x arch/install
chmod +x modules/* 
chmod +x modules/post/*
./arch/install
pacman -Syy
which dialog || pacamn -S dialog
which curl || pacman -S curl
which git || pacman -S git
which mkfs.fat || pacman -S dosfstools
setfont ter-v16n || pacman -S terminus-font && setfont ter-16n

clear
echo "Установка QuasarLinux REV-1"
sleep 2
ls modules/

bash modules/install
